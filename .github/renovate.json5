{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'github>MichaelSp/MichaelSp//.github/renovate.json5',
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^Chart\\.yaml$/',
      ],
      matchStrings: [
        '^appVersion:\\s*"?(?<currentValue>\\d+\\.\\d+\\.(?:\\d+)?)"?$',
      ],
      datasourceTemplate: 'github-releases',
      versioningTemplate: 'semver',
      depNameTemplate: 'home-assistant/core',
    },
  ],
}
